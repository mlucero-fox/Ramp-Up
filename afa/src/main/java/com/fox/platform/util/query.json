{
  "query": {
    "bool": {
      "must": [{
        "term": {
          "type.description": "olympicschannel"
        }
      },
      {
        "nested": {
          "path": "groups",
          "query": {
            "bool": {
              "must": [{
                "nested": {
                  "path": "groups.feeds",
                  "query": {
                    "match": {
                      "groups.feeds.countryId": "<<CountryId>>"
                    }
                  }
                }
              }]
            }
          },
          "inner_hits": {
             
          }
        }
      }]
    }
  },
  "sort": [{
    "groups.fields.id.raw": {
      "nested_path": "groups",
      "order": "asc"
    }
  }]
}